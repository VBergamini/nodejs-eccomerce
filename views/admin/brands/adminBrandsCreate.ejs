<div>
  <h1>Create Brand</h1>
</div>
<div>
  <form>
    <div class="form-group">
      <label for="inputName">Brand Name</label>
      <input type="text" class="form-control" id="inputName" />
    </div>
    <div class="form-group">
      <label for="inputImage">Brand Image</label>
      <input class="form-control" type="file" id="inputImage" />
    </div>
    <div style="padding:10px;">
      <img alt="Product Image" style="display:none;" width="200" id="imagePreview">
    </div>
    <div>
      <button class="btn btn-primary" type="button" id="btnCreate">Create</button>
      <a href="/brands" class="btn btn-default">Back</a>
    </div>
  </form>
</div>
<script src="/js/admin/brands/create.js"></script>