                    <a href="/store/cart">
                      <i class="icon icon-shopping-cart"></i>
                    </a>
                  </li>
                  <li>
                    <a href="/store/wishlist">
                      <i class="icon icon-heart"></i>
                    </a>
                  </li>
                  <li class="user-items search-item pe-3">
                    <a href="#" class="search-button">
                      <i class="icon icon-search"></i>
                    </a>
                  </li>
                </ul>

              </div>
            </div>
          </div>
        </nav>

        <nav class="primary-nav padding-small">
          <div class="container">
            <div class="row d-flex align-items-center">
              <div class="col-lg-2 col-md-2">
                <div class="main-logo">
                  <a href="/store"><img src="/img/store/logo/logo.png" alt="logo"></a>
                </div>
              </div>
              <div class="col-lg-10 col-md-10">
                <div class="navbar">
                  <div id="main-nav" class="stellarnav d-flex justify-content-end right">

                    <ul class="menu-list">
                      <li class="menu-item has-sub">
                        <a href="/store" class="item-anchor d-flex align-item-center" data-effect="Home">Home</a>
                      </li>
                      <li class="menu-item has-sub">
                        <a href="/store/categories/" class="item-anchor d-flex align-item-center" data-effect="Categories">Categories<i class="icon icon-chevron-down"></i></a>
                        <ul class="submenu">
                          <% categoryList.map(function(value, index) { %>
                          <li>
                            <a href="/store/categories/<%= value.categoryId %>" id="<%= value.categoryId %>" class="selectedCategory"><%= value.categoryName %></a>
                          </li>
                          <% }) %>
                        </ul>
                      </li>
                      <li class="menu-item has-sub">
                        <a href="/store/brands" class="item-anchor d-flex align-item-center" data-effect="Brands">Brands<i class="icon icon-chevron-down"></i></a>
                          <ul class="submenu">
                            <% brandList.map(function(value, index) { %>
                            <li>
                              <a href="/store/brands/<%= value.brandId %>" id="<%= value.brandId %>" class="item-anchor selectedBrand"><%= value.brandName %></a>
                            </li>
                            <% }) %>
                          </ul>
                      </li>
                    </ul>

                  </div>
                </div>
              </div>
            </div>
          </div>
        </nav>
      </div>
